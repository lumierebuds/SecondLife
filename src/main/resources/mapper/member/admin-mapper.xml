<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin">
	
	<select id="adminLogin" resultType='member'>
		SELECT 
			*
		FROM MEMBER
		WHERE ID = #{id}
		  AND ADMIN_AUTH = 'Y'
	</select>
	
</mapper>